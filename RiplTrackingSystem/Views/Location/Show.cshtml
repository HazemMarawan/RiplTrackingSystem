            <li class="breadcrumb-item"><a href="@Url.Action("Index","Location")">Locations</a></li>
            @foreach (var path in ViewBag.Paths)
            {
                <li class="breadcrumb-item active"><a href="/Location/Show?id=@path.id">@path.name</a></li>
            }

        </ol>
    </nav>
