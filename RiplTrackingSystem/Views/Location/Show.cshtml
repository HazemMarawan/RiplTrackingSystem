@model RiplTrackingSystem.ViewModel.CompanyDetail
@{
    ViewBag.Title = "Show";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var male_id = (int)Gender.Male;
    var female_id = (int)Gender.Female;

    string factoryActive = String.Empty;
    string storeActive = String.Empty;
    string distributorActive = String.Empty;

    string factoryShowActive = String.Empty;
    string storeShowActive = String.Empty;
    string distributorShowActive = String.Empty;

    if (Model.type == 1)
    {
        factoryActive = "active";
        factoryShowActive = "show active";
    }

    if (Model.type == 2)
    {
        storeActive = "active";
        storeShowActive = "show active";
    }

    if (Model.type == 3)
    {
        distributorActive = "active";
        distributorShowActive = "show active";
    }
}
<div class="modal fade" id="manage_location_modal" tabindex="-1" role="dialog" aria-hidden="true">
    <form action="" method="post" enctype='multipart/form-data'>
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="modal_title"></h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="form-body">
                        <div class="form-row">
                            <input type="hidden" name="id" id="id" value="0" />
                            <div class="form-group col-md-6">
                                <label>Name</label>
                                <input required name="name" id="name" type="text" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Description</label>
                                <input type="text" name="description" id="description" class="form-control radius-30" />
                            </div>

                            <input type="hidden" name="id" id="id" value="0" />
                            <div class="form-group col-md-6">
                                <label>Address</label>
                                <input required name="address" id="address" type="text" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Phone</label>
                                <input type="text" name="phone" id="phone" class="form-control radius-30" />
                            </div>
                            <input type="hidden" id="parent_id" name="parent_id" />
                            <input type="hidden" id="type" name="type" />
                        </div>
                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div>
    </form>
</div>

<div class="modal fade" id="manage_user_modal" tabindex="-1" role="dialog" aria-hidden="true">
    <form action="" method="post" enctype='multipart/form-data'>
        <div class="modal-dialog  modal-lg">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title" id="modal_title"></h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="form-body">
                        <div class="form-row">
                            <input type="hidden" name="id" id="id" value="0" />
                            <div class="form-group col-md-12">
                                <center><img src="~/images/userProfile.png" width="150" height="150" id="imageProfile" class="rounded-circle shadow" alt="" /></center>
                            </div>
                            <div class="form-group col-md-6">
                                <label>Username <small id="user_name_error" style="display:none;" class="badge badge-danger"> <i class="lni lni-close"></i> username already taken</small><small id="user_name_valid" class="badge badge-success" style="display:none;"><i class="lni lni-checkmark"></i> valid username</small></label>
                                <input required name="user_name" id="user_name" type="text" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-6">
                                <label>Password</label>
                                <input required name="password" id="password" type="text" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-12">
                                <label>Full Name</label>
                                <input required name="full_name" id="full_name" type="text" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-4">
                                <label>Code <small id="code_error" style="display:none;" class="badge badge-danger"> <i class="lni lni-close"></i> code already taken</small><small id="code_valid" class="badge badge-success" style="display:none;"><i class="lni lni-checkmark"></i> valid code</small></label>
                                <input required name="code" id="code" type="text" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-4">
                                <label>Email</label>
                                <input type="text" name="email" id="email" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-4">
                                <label>Phone</label>
                                <input type="number" name="phone1" id="phone1" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-4">
                                <label>Address</label>
                                <input type="text" name="address1" id="address1" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-4">
                                <label>BirthDate</label>
                                <input type="date" name="birthDate" id="birthDate" class="form-control radius-30" />
                            </div>
                            <div class="form-group col-md-4">
                                <label>Gender</label>
                                <select name="gender" id="gender" class="form-control radius-30">
                                    <option value="@male_id">@Gender.Male</option>
                                    <option value="@female_id">@Gender.Female</option>
                                </select>
                            </div>
                            <div class="form-group col-md-12">
                                <label>Role</label>
                                @Html.DropDownList("role_ids", new SelectList(ViewBag.roles, "id", "name"), "Select Role", new { @class = "selectize", @multiple = "multiple", @name = "role_ids", @style = "width:100%", @id = "role_ids" })

                            </div>
                            <input type="hidden" name="location_id" id="location_id" />

                            <div class="form-group col-md-6">
                                <label>Personal Photo</label> <br />
                                <input type="file" onchange="loadFile(event)" class="form-control" id="image" name="image" aria-describedby="inputGroupFileAddon01">
                            </div>

                            <div class="form-group col-md-6">
                                <label>Status</label>
                                <select class="form-control" name="active" id="active">
                                    <option value="1">Active</option>
                                    <option value="2">Inactive</option>
                                </select>
                            </div>
                        </div>


                    </div>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn" data-dismiss="modal">Close</button>
                    <button type="submit" class="btn btn-primary">Save changes</button>
                </div>
            </div>
        </div>
    </form>
</div>

<div class="page-header">
